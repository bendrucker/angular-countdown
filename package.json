{
  "name": "angular-countdown",
  "version": "1.2.1",
  "description": "Radial countdown timer for Angular",
  "main": "./src",
  "scripts": {
    "test": "karma start --single-run",
    "example": "browserify -r ./:angular-countdown > ./example/bundle.js && opn ./example/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bendrucker/angular-countdown.git"
  },
  "keywords": [
    "angular",
    "countdown",
    "radial",
    "animated"
  ],
  "author": "Ben Drucker <bvdrucker@gmail.com> (http://www.bendrucker.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bendrucker/angular-countdown/issues"
  },
  "homepage": "https://github.com/bendrucker/angular-countdown",
  "dependencies": {
    "angular": "^1.3.0",
    "babelify": "~10.0.0",
    "browserify-shim": "~3.8.2",
    "insert-css": "~2.0.0",
    "partialify": "~3.1.3"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim",
      "partialify"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular"
  },
  "devDependencies": {
    "angular-mocks": "~1.7.5",
    "browserify": "~16.2.3",
    "chai": "~4.2.0",
    "karma": "~3.1.1",
    "karma-browserify": "~5.3.0",
    "karma-cli": "1.0.1",
    "karma-mocha": "~1.3.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "mocha": "~5.2.0",
    "opn": "~5.4.0",
    "sinon": "~7.1.1"
  }
}
